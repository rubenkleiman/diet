{
  "name": "diet-guidelines",
  "type": "module",
  "version": "1.0.0",
  "description": "Diet Guidelines Application",
  "main": "app.js",
  "scripts": {
    "dev": "nodemon --inspect=9229 app.js",
    "watch": "esbuild public/js/client.js --bundle --outfile=public/js/bundle.js --sourcemap --format=iife --watch",
    "dev:full": "concurrently \"npm run dev\" \"npm run watch\"",
    "build": "esbuild public/js/client.js --bundle --outfile=public/js/bundle.js --sourcemap --format=iife",
    "db:comment": "echo 'db:* scripts handle db backup & population'",
    "db:init": "node db/scripts/init.js",
    "db:reset": "node db/scripts/init.js --reset",
    "db:test": "node db/scripts/test.js",
    "db:populate": "node db/scripts/Populate.js",
    "db:backup": "node db/scripts/Backup.js",
    "db:backup:list": "node db/scripts/Backup.js --list",
    "db:restore": "node db/scripts/Backup.js --restore"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "connect-livereload": "^0.6.1",
    "css-loader": "^6.8.1",
    "dead-code-checker": "^1.1.0",
    "esbuild": "^0.27.0",
    "knip": "^5.70.0",
    "livereload": "^0.10.3",
    "nodemon": "^3.0.1",
    "skott": "^0.35.6",
    "style-loader": "^3.3.3"
  },
  "dependencies": {
    "express": "^5.1.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.6"
  }
}
